{
    "compilerOptions": {
      "target": "ES6",
      "module": "commonjs",
      "rootDir": "./",
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["server.ts", "src/**/*"],
    "exclude": ["node_modules"]
  }
  